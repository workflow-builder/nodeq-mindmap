{
  "name": "nodeq-mindmap",
  "version": "2.0.0",
  "description": "Enhanced mind map visualization library with ML-powered data pipeline capabilities",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "bin": {
    "nodeq-mindmap": "dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "echo 'Tests will be added in future versions' && exit 0",
    "lint": "echo 'Linting will be added in future versions' && exit 0",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mindmap",
    "visualization",
    "d3",
    "data-pipeline",
    "machine-learning",
    "typescript",
    "interactive",
    "svg",
    "cli"
  ],
  "author": "NODEQ Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/workflow-builder/nodeq-mindmap.git"
  },
  "homepage": "https://workflow-builder.github.io/nodeq-mindmap",
  "bugs": {
    "url": "https://github.com/workflow-builder/nodeq-mindmap/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "peerDependencies": {
    "d3": "^7.0.0"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "@tensorflow/tfjs": "^4.21.0",
    "jsdom": "^25.0.1",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "typescript": "^5.6.2",
    "rollup": "^4.22.4",
    "@rollup/plugin-typescript": "^12.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "tslib": "^2.7.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}